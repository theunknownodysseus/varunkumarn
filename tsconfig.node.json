{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ES2015",
    "lib": ["ES2023"],
    "types": ["node"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["vite.config.ts", "src/server"]
}
